<aura:component implements="lightning:isUrlAddressable,force:hasRecordId,lightning:actionOverride,lightning:hasPageReference">
    <aura:attribute name="selectedRecordId" type="Id" />
     
    <!--Declare Handler-->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>  

    <c:createOpportunity accountId="{!v.recordId}" recordTypeId="{!v.selectedRecordId}"></c:createOpportunity>
</aura:component>